{
  "manifest_version": 3,
  "name": "Ritual",
  "description": "Daily commitment dashboard.",
  "version": "1.0",

  "permissions": ["storage", "sidePanel"],

  "action": {
    "default_title": "Open Ritual"
  },

  "background": {
    "service_worker": "background.js"
  },

  "side_panel": {
    "default_path": "newtab.html"
  },

  "icons": {
    "16": "icons/image.jpg",
    "48": "icons/image (1).jpg",
    "128":"icons/image (2).jpg"
  }
}
