{
  "manifest_version": 3,
  "name": "Ritual",
  "description": "Daily commitment dashboard that replaces your new tab.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16" : "icons/image.jpg",
    "48" : "icons/image (1).jpg",
    "128": "icons/image (2).jpg"
  }
}
